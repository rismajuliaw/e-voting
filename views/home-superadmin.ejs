<!DOCTYPE html>
<head>
    <title>Super Admin</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link type="text/css" rel="stylesheet" href="/assets/css/homesuperadm.css">
    <link rel="stylesheet" media="screen" href="/assets/css/homesuperadm.css" />
    <!-- Bootstrap & JS -->
      <link rel="stylesheet" href="/assets/vendor/bootstrap/css/bootstrap.min.css">
      <script type="text/javascript" src="/assets/script/jquery.js"></script>
      <script type="text/javascript" src="/assets/vendor/bootstrap/js/bootstrap.min.js"></script>
      <script type="text/javascript" src="/assets/script/popper.js"></script>
</head>
<body>
            <div class="wrapper">
                <!-- Sidebar -->
                <nav id="sidebar">
                    <div class="sidebar-header">
                        <img src="/assets/img/logofix.png" style="height: 50px;">
                    </div>

                    <ul class="list-unstyled components">
                        <li class="active">
                            <a href="#"><ion-icon name="home-outline" class="col-1" style="height: 22px;"></ion-icon>Home</a>
                        </li>
                        <li>
                            <a href="#"><ion-icon name="thumbs-up-outline" class="col-1" style="height: 22px;"></ion-icon>Vote</a>
                        </li>
                        <li>
                            <a href="/super/Logout"><ion-icon name="log-out-outline" class="col-1" style="height: 22px;"></ion-icon>Logout</a>
                        </li>
                    </ul>
                </nav>
                <div id="content">
                    <nav class="navbar navbar-expand-lg navbar-light bg-light">
                        <div class="container-fluid">
                            <button type="button" id="sidebarCollapse" class="btn">
                                <img src="/assets/img/menu.png">
                            </button>

                            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                                <ul class="nav navbar-nav ml-auto">
                                    <li class="nav-item" style="color:#7E8DA4;">
                                        <img src="/assets/img/pemilih.png" style="height: 30px;" class="rounded-circle">
                                        <%= name %>
                                    </li>
                            </div>
                        </div>
                    </nav> 
                    <div class="container-fluid">
                        <div class="row">
                                <div class="h-100 w-100 d-none d-lg-block col-lg-4">
                                    <div id="carouselCon" class="carousel-container">
                                        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                        <ol class="carousel-indicators">
                                            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                        </ol>
                                        <div class="carousel-inner">
                                            <div class="carousel-item active" id="caro1">
                                            <div class="container1">
                                                <img src="/assets/img/Frame 1.v1.png">
                                            </div>
                                            </div>
                                            <div class="carousel-item">
                                            <div class="container2">
                                                <img src="/assets/img/Frame 2.v1.png">
                                            </div>
                                            </div>
                                        </div>
                                        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <div id="card-con" >
                        <div class="row">
                            <div class="p-4 cards-rule">
                            <li>
                                <a href="#">
                                    <div class="shadow card card1" style="width: 20rem;">
                                        <img class="card-img-top" style="height: 100px; width: 100px; text-align: center;" src="/assets/img/tatacara.png" alt="Card logo event">
                                        <div class="p-4 card-body">
                                            <h3 class="card-title" style="text-align: center;">TATA CARA</h3>
                                            <div class="col-md-12">
                                                <div class="form-row text-paslon1">
                                                <div class="col-md-6">
                                                    <p class="card-text">Tata cara dalam melakukan voting</p>
                                                </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="/super/events">
                                    <div class="shadow card card2" style="width: 20rem;">
                                        <img class="card-img-top" style="height: 100px; width: 100px; text-align: center;" src="/assets/img/calendar.png" alt="Card logo event">
                                        <div class="p-4 card-body">
                                            <h3 class="card-title" style="text-align: center;">ACARA</h3>
                                            <div class="col-md-12">
                                                <div class="form-row text-paslon1">
                                                <div class="col-md-6">
                                                    <p class="card-text">Daftar acara yang sedang berlangsung</p>
                                                </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <div class="shadow card card3" style="width: 20rem;">
                                        <img class="card-img-top" style="height: 100px; width: 100px; text-align: center;" src="/assets/img/tips.png" alt="Card logo event">
                                        <div class="p-4 card-body">
                                            <h3 class="card-title" style="text-align: center;">TIPS</h3>
                                            <div class="col-md-12">
                                                <div class="form-row text-paslon1">
                                                <div class="col-md-6">
                                                    <p class="card-text">Tips sebelum melakukan voting</p>
                                                </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </li>
                            </div>
                        </div>
                        <div class="row">
                                <div class="col-md-8 head-project">
                                    <h3>E-VOTING PROJECT TEAM</h3>
                                </div>
                        </div>
                        <div class="row">
                            <div class="p-5 member-card">
                                <li>
                                <div class="card mem-card" style="width: 20rem;">
                                    <img class="card-img-top" style="height: 100px; width: 100px; text-align: center;" src="/assets/img/icon bagas.png" alt="Card logo event">
                                    <div class="card-body bagas">
                                        <h4 class="card-title">Bagas Tri Wibowo</h4>
                                        <div class="col-md-12">
                                            <div class="form-row">
                                            <div class="col-md-6">
                                                <p class="card-text">as Back End</p>
                                            </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                </li>
                                <li>
                                    <div class="card mem-card" style="width: 20rem;">
                                        <img class="card-img-top" style="height: 100px; width: 100px; text-align: center;" src="/assets/img/icon rahma.png" alt="Card logo event">
                                        <div class="card-body rahma">
                                            <h4 class="card-title">Rahma Fadhilah</h4>
                                            <div class="col-md-12">
                                                <div class="form-row">
                                                <div class="col-md-6">
                                                    <p class="card-text">as UI/UX</p>
                                                </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li>
                                    <div class="card mem-card" style="width: 20rem;">
                                        <img class="card-img-top" style="height: 100px; width: 100px; text-align: center;" src="/assets/img/icon risma.png" alt="Card logo event">
                                        <div class="card-body risma">
                                            <h4 class="card-title">Risma Julia W</h4>
                                            <div class="col-md-12">
                                                <div class="form-row">
                                                <div class="col-md-6">
                                                    <p class="card-text">as Front End</p>
                                                </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </div>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
                
    <script src="https://unpkg.com/ionicons@5.1.2/dist/ionicons.js"></script>
    <script type="text/javascript">
    $(document).ready(function () {

        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
            $('#carouselCon').toggleClass('active');
            $('#card-con').toggleClass('active');
        });
    
    });
    </script>

</body>
</html>