<!DOCTYPE html>
<head>
    <title>Dashboard Admin</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link type="text/css" rel="stylesheet" href="http://localhost:8888/assets/css/dbsuperadm1.css">
    <link rel="stylesheet" media="screen" href="http://localhost:8888/assets/css/dbsuperadm1.css" />
    <!-- Bootstrap -->
      <link rel="stylesheet" href="http://localhost:8888/assets/vendor/bootstrap/css/bootstrap.min.css">
      <script type="text/javascript" src="http://localhost:8888/assets/script/jquery.js"></script>
      <script type="text/javascript" src="http://localhost:8888/assets/vendor/bootstrap/js/bootstrap.min.js"></script>
      <script type="text/javascript" src="http://localhost:8888/assets/script/popper.js"></script>
      <script type="text/javascript" src="http://localhost:8888/assets/navsidebar.js"></script>
</head>
<body>
    <div id="sidebar">
                <button>
                    <div class="toggle-btn" onClick="toggleSidebar()">
                        <span></span>
                        <span></span>
                        <span></span>
                    </div>
                </button> 
                    <div class="p-3">
                        <button class="cards">
                            <div class="shadow card" style="width: 18rem;">
                                <img class="card-img-top" src="http://localhost:8888/assets/img/permif.png" alt="Card logo event">
                              </div>
                        </button>
                    </div>
            <div class="navbar-brand">
                <img src="http://localhost:8888/assets/img/logofix.png">
            </div>
            <ul>
                <li class="active">
                    <img src="http://localhost:8888/assets/img/event.png">
                    <a href="#">Daftar Event</a>
                </li>
                <li>
                    <img src="http://localhost:8888/assets/img/admin.png">
                    <a href="#">Daftar Admin</a>
                </li>
                <li>
                    <img src="http://localhost:8888/assets/img/logout.png">
                    <a href="/adm/auth/logout">Logout</a>
                </li>
            </ul>
    </div>
    <div id="content">
        <div class="shadow-sm container-fluid bg-top">
            <div class="col-md auto">
            <div class="superadm-btn">
                <img src="http://localhost:8888/assets/img/superadmin.jpeg" class="rounded-circle">
                <div class="superadmalt">
                    <%= name %>
                </div>
            </div>
        </div>
        </div>
                
    </div>
    <script type="text/javascript">
        function toggleSidebar() {
        document.getElementById("sidebar").classList.toggle('active');
        }
    </script>
</body>
</html>